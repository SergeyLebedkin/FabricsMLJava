<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.VBox?>


<ScrollPane prefHeight="429.0" prefWidth="624.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.ingrain.fabrics.ui.SelectWindowController">
   <content>
      <VBox fx:id="mainPanel">
         <children>
            <Button fx:id="buttonScaleDown" mnemonicParsing="false" onMousePressed="#buttonScaleDownClick" prefHeight="25.0" prefWidth="395.0" text="-" />
            <Button fx:id="buttonScaleUp" mnemonicParsing="false" onMousePressed="#buttonScaleUpClick" prefHeight="25.0" prefWidth="394.0" text="+" />
            <Button fx:id="buttonSelectImage" mnemonicParsing="false" onMousePressed="#buttonSelectImageClick" prefHeight="25.0" prefWidth="392.0" text="Select image" />
         </children>
      </VBox>
   </content>
</ScrollPane>
